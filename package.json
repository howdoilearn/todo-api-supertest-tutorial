{
  "name": "todo-api-supertest-tutorial",
  "version": "1.0.0",
  "description": "Quality-first Todo API with comprehensive Supertest integration testing",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --verbose --runInBand",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage --runInBand"
  },
  "keywords": [
    "api",
    "todo",
    "supertest",
    "testing",
    "express",
    "sqlite",
    "jwt"
  ],
  "author": "Ian",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
